<<mjml>
  <mj-head>
  <mj-title>Groupon For You</mj-title>
    <mj-attributes>
      <mj-text font-family="Arial" font-size="18px" line-height="20px" padding="0" />
      <mj-image padding="0px" />
      <mj-button background-color="#53a318" font-size="12px" color="#ffffff" font-family="sans-serif" border-radius="20px" height="45px" width="105px" />
      <mj-section background-color="#ffffff" padding="0" />
    </mj-attributes>
    <mj-style>
      * {
        box-sizing: border-box;
      }

      a {
        text-decoration: none;
        color: #ffffff;
        font-weight: bold;
        font-size: 14px!important;
      }

      a:hover {
        color: #000000;
      }

      @media only screen and (max-width:390px){
        .price-deal {
            padding: 0 3px 1px 2px!important;
        }
        span.price-one{
            font-size: 13px!important;
        }
        span.price-two{
            font-size: 15px!important;
        }
        .deal-description{
            padding-bottom: 0!important;
        }
       
      }
    </mj-style>
    <mj-style inline="inline">
      .header {
        background-color: #ffffff;
        text-align: center;
      }

      .border-gray {
        border-bottom: 1px solid #303030;
        border-right: 1px solid #303030;
        border-left: 1px solid #303030;
      }

      .full-border {
        border: 1px solid #303030;
      }

      .line-through {
        text-decoration: line-through;
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#ffffff">
    <mj-include path="./componets/header.mjml" />
    <!--start top section-->
    <mj-section background-color="#ffffff" padding="0">
      <mj-column width="50%" padding="5px 2px">
        <mj-image css-class="border-gray" src="https://i.imgur.com/dbMgvpQ.png" href="https://groupon.com" alt="Maryland Zoo" width="292px" height="308px" padding="0" />
      </mj-column>
      <mj-column width="50%" padding="5px 2px">
        <mj-image css-class="border-gray" src="https://i.imgur.com/vfmtJfb.png" href="https://groupon.com" alt="Image of a Bar" width="292px" height="308px" padding="0" />
      </mj-column>
    </mj-section>

    <mj-section background-color="#ffffff" padding="0">
      <mj-column width="50%" padding="5px 2px">
        <mj-image css-class="border-gray" src="https://i.imgur.com/M26ZYbf.png" href="https://groupon.com" alt="Aladdin Hotel" width="292px" height="308px" padding="0" />
      </mj-column>
      <mj-column width="50%" padding="5px 2px">
        <mj-image src="https://i.imgur.com/oJKehhR.png" href="https://groupon.com" alt="Link to to do list" width="292px" height="200px" padding="0" />
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff">
      <mj-column padding="10px 0px">
        <mj-button css-class="big-btn-link" href="https://groupon.com" width="300px" line-height="100%">
          See All my Recently Viewed Deals
        </mj-button>
      </mj-column>
    </mj-section>
    <!--end top section-->
    <!--start second section1-->
    <mj-section padding="5px 2px 0">
      <mj-group css-class="full-border">
        <mj-column padding="10px 0 10px">
          <mj-image src="https://i.imgur.com/dUvSDMY.jpg" alt="Snickets Water Park" width="260px" height="220px" />
        </mj-column>
        <mj-column>
          <mj-text css-class="deal-title" padding-top="3px" font-size="16px">
            Snickets Water Park
          </mj-text>
          <mj-text css-class="deal-subtitle" font-size="12px" padding="5px 3px 0px 0px">
            Coastal Water Park
          </mj-text>
          <mj-text css-class="deal-description" padding="10px 3px 10px 0px" font-size="11px">
            Enjoy a fun summer day off the coast of sunny Florida at Snickets Water Park. Or enjoy the dry side and try our massive roller coasters and other fun rides.
          </mj-text>
          <mj-text css-class="price-deal" padding="10px 6px" align="left">
            <span class="price-one" style="text-decoration: line-through; padding-right: 10px; padding-bottom: 7px; font-family:arial; font-size:16px;">$59.95 </span>
            <span class="price-two" style="padding-right: 15px; padding-bottom: 7px; font-family: arial; font-size: 18px; color:#53a318; font-weight:bold;"> $39.95</span>
          </mj-text>
          <mj-text css-class="view-deal" align="right">
            <a href="https://groupon.com" style="background-color:#53a318;padding:5px 15px; border-radius:20px; font-weight: normal; Margin-right: 3px;">View Deal</a>
          </mj-text>
        </mj-column>
      </mj-group>
    </mj-section>
    <!--start second section2-->
    <mj-section padding="0px 2px">
      <mj-group css-class="border-gray">
